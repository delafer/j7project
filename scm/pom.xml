			<email>weblogic@bk.ru</email>
			<roles>
				<role>Developer</role>
				<role>Architect</role>
				<role>Technical Project Lead</role>
				<role>Chief of everything</role>
			</roles>
			<timezone>Europe/Berlin</timezone>
