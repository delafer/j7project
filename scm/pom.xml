		<developer>
			<id>andrek</id>
			<name>Andrey Korestalev</name>
			<email>akorestalev@gmail.com</email>
			<roles>
				<role>Developer</role>
				<role>Tester</role>
			</roles>
			<timezone>Europe/Moscow</timezone>
		</developer>
	</developers>
	<contributors>
		<contributor>
			<name>Alexandr Tavrovsky</name>
			<email>weblogic@bk.ru</email>
			<roles>
				<role>Project Lead</role>
			</roles>
			<timezone>Europe/Stuttgart</timezone>
		</contributor>
	</contributors>
</project>
